{
	"name": "nella/framework",
	"type": "nette-addon",
	"description": "Nella Framework",
	"keywords": ["nella", "framework", "doctrine", "nette"],
	"homepage": "http://nellafw.org",
	"license": ["BSD-3", "GPLv2", "GPLv3"],
	"authors": [
		{
			"name": "Patrik Votoček",
			"email": "patrik@votocek.cz",
			"homepage": "http://patrik.votocek.cz"
		},
		{
			"name": "Nella Project",
			"homepage": "http://nella-project.org"
		}
	],
	"repositories": [
		{
			"type": "pear",
			"url": "http://pear.nette.org"
		}
	],
	"autoload": {
		"psr-0": { "Nella": "Nella/" }
	},
	"config": {
		"vendor-dir": "vendors"
	},
	"require": {
		"php": ">=5.3.2",
		"doctrine/orm": "2.2.*",
		"doctrine/migrations": "*",
		"symfony/console": "2.0.*",
		"beberlei/DoctrineExtensions": "*",
		"pear-nette/Nette": "2.0.1"
	},
	"replace": {
		"nella/nette-addon-doctrine": "self.version",
		"nella/nette-addon-media": "self.version",
		"nella/nette-addon-forms": "self.version"
	}
}
